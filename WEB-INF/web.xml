<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
 <servlet>
    <servlet-name>Cart</servlet-name>
    <servlet-class>CartPage</servlet-class>
 </servlet>

 <servlet-mapping>
    <servlet-name>Cart</servlet-name>
    <url-pattern>/cart/*</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>Star</servlet-name>
    <servlet-class>StarPage</servlet-class>
 </servlet>

 <servlet-mapping>
    <servlet-name>Star</servlet-name>
    <url-pattern>/star/*</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>Main</servlet-name>
    <servlet-class>MainPage</servlet-class>
 </servlet>

 <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/main/*</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>Film</servlet-name>
    <servlet-class>FilmPage</servlet-class>
 </servlet>

 <servlet-mapping>
    <servlet-name>Film</servlet-name>
    <url-pattern>/filmpage/*</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>LoginPage</servlet-class>
 </servlet>

 <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login/*</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>Profile</servlet-name>
    <servlet-class>ProfilePage</servlet-class>
 </servlet>

 <servlet-mapping>
    <servlet-name>Profile</servlet-name>
    <url-pattern>/profile/*</url-pattern>
</servlet-mapping>
</web-app>
